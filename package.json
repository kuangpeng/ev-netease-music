{
  "name": "ev-netease-music",
  "version": "1.0.0",
  "description": "netease music build by vue an electron",
  "main": "./out/main/index.js",
  "author": "kuangpeng",
  "homepage": "https://github.com/alex8088/electron-vite",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "serve": "vite --config electron.vite.config.server.ts",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.11.22",
    "@unocss/preset-uno": "^0.48.4",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vueuse/core": "^9.10.0",
    "axios": "^1.2.2",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.7.0",
    "less": "^4.1.3",
    "modern-css-reset": "^1.4.0",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.28",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
    "typescript": "^4.9.3",
    "unocss": "^0.48.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "vfonts": "^0.0.3",
    "vite": "^3.2.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  }
}
